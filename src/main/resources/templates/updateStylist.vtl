<div class="row">
  <div class="col-md-6">
    <h2>Update $stylist.getName()</h2>
    <hr>
    <form action="/stylists/$stylist.getId()/updated" method="post">
      <div class="form-group">
        <label for="update">Update Stylist Name</label>
        <input id="updateName" type="text" name="update" class="form-control" placeholder="enter new name" required>
      </div>
      <button type="submit" class="btn btn-default">Update</button>
    </form>
  </div>
</div>
